{
  "name": "prospectaai",
  "version": "1.0.0",
  "description": "Um CRM e controlado de prospecção e vendas leve, simples e direto ao ponto.",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:down": "docker compose -f infra/compose.yaml down",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:wait:database": "node infra/scripts/wait-for-postgres",
    "migrations:create": "node-pg-migrate -m infra/migrations create",
    "migrations:up": "node-pg-migrate -m infra/migrations --envPath .env.development up",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matheushnascimento/prospectaai.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matheushnascimento/prospectaai/issues"
  },
  "homepage": "https://github.com/matheushnascimento/prospectaai#readme",
  "dependencies": {
    "cookie": "^1.0.2",
    "dotenv": "^17.2.2",
    "dotenv-expand": "^12.0.3",
    "next": "^15.5.2",
    "next-connect": "^1.0.0",
    "node-pg-migrate": "7.6.1",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "swr": "^2.3.6"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "prettier": "^3.6.2"
  }
}
